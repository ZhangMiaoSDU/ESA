/**app.wxss**/
.navBar { 
  background: #fff;
  position: fixed;
  top: 0;
}

.navBar .left {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100%;
  width: 100%;
  /* padding: 5rpx 10px; */
}

.btn-view {
  height: 88rpx;
  line-height: 80rpx;
  background: rgba(246, 246, 246, .96);
  display: flex;
  justify-content: center;
  align-items: center;
}

.btn {
  text-align: center;
  width: 100%;
  background: rgba(219, 244, 224);
  color: rgb(15, 177, 72);
  border-radius: 30rpx;
  font-weight: 400
}


.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

@font-face {
    font-family: 'icon';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}

.icon-back {
  font-family:"icon" !important;
  font-size:32rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-back:before { content: "\e600"; }
.back-btn {
	padding: 0 20rpx;
}


.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.1);
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;

}

.tabs {
  display: flex;
  justify-content: space-between;
}

.tabs .tabs-item {
  width: 50%;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx 0 0 0;
  color: rgb(15, 177, 72);
}

.tabs .tabs-item:last-child {
  border-radius: 0 20rpx 0 0;
}

.tabs-item-active-external {
  background: #ddf4df
}


.table, .table div {
  margin: 0 auto;
}
.table {
  display: table;
  width: 100%;
  border-collapse: collapse; 
  box-sizing: border-box;
  font-size: 30rpx;
}

.table-tr {
  display: table-row; 
  height: 72rpx;
  width: 100%;
  color: #2c2c2c
}

.table-th {
  display: table-cell;
  font-weight: bold;
  border: 4rpx solid #f6f6f6;
  text-align: center;
  vertical-align: middle;
  border-top: None;
  border-right: None;
  border-left: None;
  /* width: 20%; */
}

.table-td {
  display: table-cell;
  border: 1rpx solid #f6f6f6;
  text-align: center;
  vertical-align: middle;
  border-right: None;
  border-left: None;
}

button {
  background: initial;
}

button:focus {
  outline: 0
}

button::after {
  border: none
}
